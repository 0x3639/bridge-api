# Application
APP_NAME=Orchestrator API
DEBUG=false
API_V1_PREFIX=/api/v1
DOCS_ENABLED=true

# Security
SECRET_KEY=change-this-to-a-secure-random-string
API_TOKEN_PREFIX=ora_

# Database
DATABASE_URL=postgresql+asyncpg://orchapi:password@localhost:5432/orchestrator

# Redis
REDIS_URL=redis://localhost:6379/0

# Rate Limiting Defaults
DEFAULT_RATE_LIMIT_PER_SECOND=10
DEFAULT_RATE_LIMIT_BURST=20
ADMIN_RATE_LIMIT_PER_SECOND=100
ADMIN_RATE_LIMIT_BURST=200

# Orchestrator Collection
ORCHESTRATOR_POLL_INTERVAL=60
ORCHESTRATOR_RPC_TIMEOUT=10
ORCHESTRATOR_RPC_PORT=55000
MIN_ONLINE_FOR_BRIDGE=16

# Cache TTLs (seconds)
CACHE_STATUS_TTL=10
CACHE_USER_TTL=300
CACHE_STATS_TTL=60
